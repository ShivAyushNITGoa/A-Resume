<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg xmlns="http://www.w3.org/2000/svg" width="400" height="400" viewBox="0 0 400 400">
  <!-- Black Background Circle -->
  <circle cx="200" cy="200" r="200" fill="black"/>
  
  <!-- Text "CAMPUS DEVELOPERS" curved around -->
  <path id="textPath" d="M 200,200 m 0,-170 a 170,170 0 1,1 -1,0" fill="none"/>
  <text fill="white" font-family="serif" font-size="24" font-weight="bold" letter-spacing="1.5">
    <textPath href="#textPath" startOffset="0%">CAMPUS DEVELOPERS</textPath>
  </text>
  
  <!-- Text "GDEVELOPERS" at bottom -->
  <text x="200" y="330" fill="#A0A0A0" font-family="sans-serif" font-size="30" text-anchor="middle">GDEVELOPERS</text>
  
  <!-- Red geometric star pattern -->
  <g transform="translate(200, 200)">
    <g fill="none" stroke="#D94A4A" stroke-width="2">
      <polygon points="0,-80 68,-40 68,40 0,80 -68,40 -68,-40" transform="rotate(0)"/>
      <polygon points="0,-80 68,-40 68,40 0,80 -68,40 -68,-40" transform="rotate(10)"/>
      <polygon points="0,-80 68,-40 68,40 0,80 -68,40 -68,-40" transform="rotate(20)"/>
      <polygon points="0,-80 68,-40 68,40 0,80 -68,40 -68,-40" transform="rotate(30)"/>
      <polygon points="0,-80 68,-40 68,40 0,80 -68,40 -68,-40" transform="rotate(40)"/>
    </g>
    
    <!-- Center circle with GD text -->
    <circle cx="0" cy="0" r="45" fill="black" stroke="#D94A4A" stroke-width="2"/>
    <text x="0" y="15" fill="#D94A4A" font-family="serif" font-size="40" font-weight="bold" text-anchor="middle">GD</text>
  </g>
</svg> 