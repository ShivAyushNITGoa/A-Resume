<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg xmlns="http://www.w3.org/2000/svg" width="320" height="400" viewBox="0 0 320 400">
  <rect width="320" height="400" fill="#f8f9fa" />
  
  <!-- Header bar -->
  <rect x="0" y="0" width="320" height="80" fill="#9c4d61" />
  
  <!-- Profile photo circle -->
  <circle cx="60" cy="40" r="30" fill="#ffffff" stroke="#ffffff" stroke-width="2" />
  
  <!-- Name and title -->
  <rect x="100" y="30" width="150" height="10" fill="#ffffff" />
  <rect x="100" y="50" width="100" height="6" fill="#ffffff" opacity="0.7" />
  
  <!-- Two column layout -->
  <rect x="0" y="80" width="100" height="320" fill="#f0f0f0" />
  
  <!-- Contact info -->
  <rect x="20" y="100" width="60" height="8" fill="#9c4d61" />
  <rect x="20" y="120" width="60" height="4" fill="#666666" />
  <rect x="20" y="130" width="50" height="4" fill="#666666" />
  <rect x="20" y="140" width="55" height="4" fill="#666666" />
  
  <!-- Skills -->
  <rect x="20" y="170" width="60" height="8" fill="#9c4d61" />
  <rect x="20" y="190" width="5" height="5" rx="2.5" fill="#9c4d61" />
  <rect x="30" y="190" width="50" height="4" fill="#666666" />
  <rect x="20" y="200" width="5" height="5" rx="2.5" fill="#9c4d61" />
  <rect x="30" y="200" width="40" height="4" fill="#666666" />
  <rect x="20" y="210" width="5" height="5" rx="2.5" fill="#9c4d61" />
  <rect x="30" y="210" width="45" height="4" fill="#666666" />
  
  <!-- Main content -->
  <rect x="120" y="100" width="180" height="8" fill="#9c4d61" />
  <rect x="120" y="120" width="180" height="4" fill="#666666" />
  <rect x="120" y="130" width="170" height="4" fill="#666666" />
  <rect x="120" y="140" width="160" height="4" fill="#666666" />
  
  <!-- Experience -->
  <rect x="120" y="170" width="180" height="8" fill="#9c4d61" />
  <rect x="120" y="190" width="120" height="5" fill="#333333" />
  <rect x="250" y="190" width="50" height="5" fill="#9c4d61" opacity="0.2" />
  <rect x="120" y="200" width="180" height="3" fill="#666666" />
  <rect x="130" y="210" width="170" height="3" fill="#666666" />
  <rect x="130" y="220" width="160" height="3" fill="#666666" />
  
  <!-- Achievements -->
  <rect x="120" y="250" width="180" height="8" fill="#9c4d61" />
  <rect x="120" y="270" width="180" height="20" rx="4" fill="#9c4d61" opacity="0.1" />
  <rect x="130" y="278" width="40" height="4" fill="#9c4d61" />
  <rect x="180" y="278" width="110" height="4" fill="#666666" />
  
  <!-- Professional label -->
  <rect x="95" y="340" width="130" height="30" rx="4" fill="#9c4d61" />
  <rect x="115" y="353" width="90" height="5" fill="#ffffff" />
</svg> 