<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg xmlns="http://www.w3.org/2000/svg" width="600" height="600" viewBox="0 0 600 600">
  <!-- Black Background Circle -->
  <circle cx="300" cy="300" r="300" fill="#000000"/>
  
  <!-- Text "CAMPUS DEVELOPERS" curved around -->
  <path id="textPath" d="M 300,300 m 0,-220 a 220,220 0 1,1 -1,0" fill="none"/>
  <text fill="#FFFFFF" font-family="serif" font-size="38" font-weight="bold" letter-spacing="2">
    <textPath href="#textPath" startOffset="5%">CAMPUS DEVELOPER</textPath>
  </text>
  
  <!-- Text "GDEVELOPERS" at bottom -->
  <text x="300" y="470" fill="#A9A9A9" font-family="sans-serif" font-size="40" font-weight="normal" text-anchor="middle">GDEVELOPERS</text>
  
  <!-- Red geometric star pattern -->
  <g transform="translate(300, 300)">
    <!-- Create the geometric pattern that looks like a spirograph/star -->
    <g fill="none" stroke="#C04A4A" stroke-width="2">
      <polygon points="0,-120 102,-60 102,60 0,120 -102,60 -102,-60" transform="rotate(0)"/>
      <polygon points="0,-120 102,-60 102,60 0,120 -102,60 -102,-60" transform="rotate(5)"/>
      <polygon points="0,-120 102,-60 102,60 0,120 -102,60 -102,-60" transform="rotate(10)"/>
      <polygon points="0,-120 102,-60 102,60 0,120 -102,60 -102,-60" transform="rotate(15)"/>
      <polygon points="0,-120 102,-60 102,60 0,120 -102,60 -102,-60" transform="rotate(20)"/>
      <polygon points="0,-120 102,-60 102,60 0,120 -102,60 -102,-60" transform="rotate(25)"/>
      <polygon points="0,-120 102,-60 102,60 0,120 -102,60 -102,-60" transform="rotate(30)"/>
      <polygon points="0,-120 102,-60 102,60 0,120 -102,60 -102,-60" transform="rotate(35)"/>
      <polygon points="0,-120 102,-60 102,60 0,120 -102,60 -102,-60" transform="rotate(40)"/>
      <polygon points="0,-120 102,-60 102,60 0,120 -102,60 -102,-60" transform="rotate(45)"/>
      <polygon points="0,-120 102,-60 102,60 0,120 -102,60 -102,-60" transform="rotate(50)"/>
    </g>
    
    <!-- Center circle with GD text -->
    <circle cx="0" cy="0" r="80" fill="black" stroke="#C04A4A" stroke-width="2"/>
    <text x="0" y="20" fill="#C04A4A" font-family="serif" font-size="90" font-weight="bold" text-anchor="middle">GD</text>
  </g>
</svg> 